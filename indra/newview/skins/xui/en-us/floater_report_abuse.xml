	     width="180">
		<combo_item name="Selectcategory" value="0">
			Select category
		</combo_item>
		<combo_item name="Intolerance" value="23">
			Intolerance
		</combo_item>
		<combo_item name="Harassment" value="24">
			Harassment
		</combo_item>
		<combo_item name="Assault" value="25">
			Assault
		</combo_item>
		<combo_item name="Disclosure" value="26">
			Disclosure
		</combo_item>
		<combo_item name="Indecency" value="27">
			Indecency
		</combo_item>
		<combo_item name="Age" value="28">
			Age
		</combo_item>
		<combo_item name="Parcel" value="29">
			Parcel
		</combo_item>
		<combo_item name="Other" value="30">
			Other
		</combo_item>
	</combo_box>
     
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-26" drop_shadow_visible="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="16"
	     mouse_opaque="true" name="abuser_name_title" v_pad="0" width="180">
		Abuser name:
	</text>
	<line_editor bevel_style="in" bg_readonly_color="0, 0, 0, 0" border_style="line"
