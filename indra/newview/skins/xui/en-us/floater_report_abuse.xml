	     font="SansSerifSmall" height="16" left="16" max_length="32"
	     mouse_opaque="true" name="abuse_location_edit"
	     text_readonly_color="1, 1, 1, 1" width="180" />
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-24" drop_shadow_visible="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="16"
	     mouse_opaque="true" name="sum_title" v_pad="0" width="80">
		Summary:
	</text>
	<line_editor bevel_style="in" bg_readonly_color="0, 0, 0, 0" border_style="line"
	     border_thickness="1" bottom_delta="-18" follows="left|top"
	     font="SansSerifSmall" height="16" left="16" max_length="64"
	     mouse_opaque="true" name="summary_edit" text_readonly_color="1, 1, 1, 1"
	     width="356" />
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-28" drop_shadow_visible="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="16"
	     mouse_opaque="true" name="dscr_title" v_pad="0" width="50">
		Details:
	</text>
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-16" drop_shadow_visible="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="32" left="64"
	     mouse_opaque="true" name="bug_aviso" v_pad="0" width="356">
		Please be specific about the date, location, nature of abuse,
relevant chat/IM text, and select the object if possible.
	</text>
	<text_editor bg_readonly_color="0, 0, 0, 0" bottom_delta="-116" embedded_items="false"
	     follows="left|top" font="SansSerifSmall" height="112" left="16"
	     max_length="800" mouse_opaque="false" name="details_edit"
	     text_readonly_color="1, 1, 1, 1" width="356" word_wrap="true" />
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-24" drop_shadow_visible="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="16"
	     mouse_opaque="true" name="incomplete_title" v_pad="0" width="342">
