	     font="SansSerifSmall" height="16" left="16" max_length="32"
	     mouse_opaque="true" name="abuser_name_edit"
	     text_readonly_color="1, 1, 1, 1" width="180" />
	<button bottom_delta="-2" font="SansSerif" halign="center" height="20" label="Select"
	     label_selected="" left_delta="190" mouse_opaque="true" name="select_abuser"
	     tool_tip="Select the name of the abuser from a list" width="100" />
	<text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-24" drop_shadow_visible="true" follows="left|top"
